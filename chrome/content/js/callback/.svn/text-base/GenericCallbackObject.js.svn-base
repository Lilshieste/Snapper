//// FILENAME:    GenericCallbackObject.js// DATE:        8/30/2008// AUTHOR:      Joe Miller//// DESCRIPTION: Defines the GenericCallbackObject, which serves as a//               container for general callback functions.//// REFERENCES:  CallbackObject.js//LSDev.Common.Javascript.GenericCallbackObject = function( callbackObject, func ) {    if( arguments.length > 0 ) {        this.base.initialize.call( this, callbackObject );    }        this.callbackFunction = func;}LSDev.Common.Javascript.GenericCallbackObject.prototype = LSDev.Common.Javascript.CallbackObject;LSDev.Common.Javascript.GenericCallbackObject.prototype.base = LSDev.Common.Javascript.CallbackObject;