// Top level namespace for all LSDev code.if( !LSDev ) {    var LSDev = {};}// Namespace containing extensions of all types.if( !LSDev.Extensions ) {    LSDev.Extensions = {};}// Namespace containing code defined for general use.if ( !LSDev.Common ) {    LSDev.Common = {};}// Namespace containing Javascript code intended for general use.if( !LSDev.Common.Javascript ) {    LSDev.Common.Javascript = {};}// Namespace containing Javascript utility code intended for general use.if( !LSDev.Common.Javascript.Util ) {    LSDev.Common.Javascript.Util = {};}if( !LSDev.Common.Javascript.Widgets ) {    LSDev.Common.Javascript.Widgets = {};}